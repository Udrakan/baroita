<Override>
<Items>

  
  <!-- Name: Tonic Liquid FIX THIS DESCRIPTION!!!!!!!!!!!!!!!!!!!!!!! -->
  <!-- Description: A medical solution that's mildly effective as a treatment for internal damage, but most commonly used as an ingredient in more complex medical compounds. -->
  <!-- *************************************************************************************************************************************************************************** -->
  <TonicLiquid name="" identifier="tonicliquid" category="Material" cargocontaineridentifier="chemicalcrate" Tags="smallitem,chem,medical" description="" useinhealthinterface="true" scale="0.3" impactsoundtag="impact_metal_light">
    <PreferredContainer primary="medcab" spawnprobability="0.5"/>
    <SuitableTreatment identifier="internaldamage" suitability="-10" />
    <SuitableTreatment identifier="damage" suitability="-10" />
    <SuitableTreatment identifier="burn" suitability="-10" />
    <SuitableTreatment identifier="opiateoverdose" suitability="-10" />
    <SuitableTreatment identifier="oxygenlow" suitability="-10" />
    <SuitableTreatment identifier="opiatewithdrawal" suitability="-10" />
    <Price locationtype="Research" buyprice="1000" />
    <Fabricate suitablefabricators="medicalfabricator" requiredtime="90">
      <RequiredSkill identifier="medical" level="45" />
      <RequiredItem identifier="calcium" />
      <RequiredItem identifier="zinc" />
      <RequiredItem identifier="potassium" />
    </Fabricate>
    <Deconstruct time="20">
      <Item identifier="calcium" copycondition="true" mincondition="0.1" />
      <Item identifier="potassium" copycondition="true" mincondition="0.1" />
      <Item identifier="zinc" copycondition="true" mincondition="0.1" />
    </Deconstruct>
    <InventoryIcon texture="Content/Items/InventoryIconAtlas.png" sourcerect="704,704,64,64" origin="0.5,0.5" />
    <Sprite texture="Medicines.png" sourcerect="138,759,63,150" depth="0.6" origin="0.5,0.5" />
    <Body width="60" height="140" density="9" />
    <MeleeWeapon canBeCombined="true" removeOnCombined="true" slots="Any,RightHand,LeftHand" aimpos="5,0" handle1="-5,0" holdangle="220" reload="1.0" msg="ItemMsgPickUpSelect">
      <RequiredSkill identifier="medical" level="72"></RequiredSkill>
      <StatusEffect type="OnFailure" target="This" Condition="-100.0" disabledeltatime="true" />
      <StatusEffect type="OnUse" target="This" Condition="-50.0" disabledeltatime="true" />
      <StatusEffect tags="medical" type="OnUse" target="Character" duration="10.0">
        <ReduceAffliction identifier="internaldamage" amount="0.5" />
      </StatusEffect>
      <StatusEffect tags="medical" type="OnUse" target="Character" disabledeltatime="true">
        <Affliction identifier="durationincrease" amount="300" />
      </StatusEffect>
      <StatusEffect tags="medical" type="OnFailure" target="Character" duration="10.0">
        <ReduceAffliction identifier="internaldamage" amount="0.25" />
      </StatusEffect>
      <StatusEffect tags="medical" type="OnFailure" target="Character" disabledeltatime="true">
        <Affliction identifier="durationincrease" amount="100" />
      </StatusEffect>
      <!-- Remove the item when fully used -->
      <StatusEffect type="OnBroken" target="This">
        <Remove />
      </StatusEffect>
    </MeleeWeapon>
  </TonicLiquid>

